using System;

namespace cw3
{
    class Program
    {
        static void Main(string[] args)
        {
            double Masa, Wzrost, BMI;
            Console.WriteLine("Napisz swoj wzrost w metrach:");
            Wzrost = double.Parse(Console.ReadLine());
            Console.WriteLine("Napisz swoja wage w kilogramach:");
            Masa = double.Parse(Console.ReadLine());
            BMI = Masa / (Wzrost * Wzrost);
            Console.WriteLine("Twoje BMI jest rowne:" + BMI);
            Console.ReadKey();
        }
    }
}
