using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Ä†w.3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Zapisz dwie liczby calkowite: ");
            int one, two;
            one = Convert.ToInt16(Console.ReadLine());
            two = Convert.ToInt16(Console.ReadLine());
            if (one % two == 0)
                Console.WriteLine("Liczba {0} jest dzielnikiem {1}.", two, one);
            else
                Console.WriteLine("Liczba {0} nie jest dzielnikiem {1}.", two, one);
            Console.ReadKey();
        }
    }
}
